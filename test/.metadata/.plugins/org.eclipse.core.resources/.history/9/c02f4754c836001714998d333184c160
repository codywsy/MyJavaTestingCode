package JavaThread;

public class MyThread2 implements Runnable{
	private int ticket = 20;
	private String name;
	public MyThread2(String name){
		this.name = name;
	}
	
	public void run(){
		for(int i=0; i<100; i++){
			if(this.ticket > 0){
				System.out.println("ticket=" + ticket--);
			}
		}
	}
}

